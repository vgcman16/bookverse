    "lodash": "^4.17.21",
    "multi-sort-stream": "^1.0.4",
    "multipipe": "^4.0.0",
    "node-ipc": "^9.2.1",
    "proper-lockfile": "^3.2.0",
    "resolve-from": "^5.0.0",
    "sanitize-filename": "^1.6.3",
    "serialize-error": "^8.1.0",
    "shell-quote": "^1.8.1",
    "signal-exit": "^3.0.7",
    "stream-json": "^1.9.1",
    "telnet-client": "^1.2.8",
    "tempfile": "^2.0.0",
    "trace-event-lib": "^1.4.1",
    "which": "^1.3.1",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1",
