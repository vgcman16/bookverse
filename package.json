{
  "name": "bookverse",
  "version": "1.0.0",
  "description": "A comprehensive book review mobile application",
  "main": "src/app.ts",
  "scripts": {
    "android": "tns run android",
    "ios": "tns run ios",
    "start": "tns run",
    "build": "tns build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:build": "detox build",
    "e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:android": "detox test --configuration android.emu.debug",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,scss}\"",
    "clean": "tns clean"
  },
  "keywords": [
    "nativescript",
    "mobile",
    "typescript",
    "book-review"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nativescript/core": "~8.5.0",
    "@nativescript/firebase-auth": "^3.3.2",
    "@nativescript/firebase-core": "^3.3.2",
    "@nativescript/firebase-messaging": "^3.3.2",
    "@nativescript/local-notifications": "^6.3.0",
    "@nativescript/theme": "~3.0.2",
    "firebase": "^11.0.2",
    "rxjs": "~7.8.1"
  },
  "devDependencies": {
    "@nativescript/android": "8.5.0",
    "@nativescript/ios": "8.5.0",
    "@nativescript/types": "~8.5.0",
    "@nativescript/webpack": "~5.0.14",
    "@types/node": "^18.19.64",
    "@types/jest": "^29.5.0",
    "@types/detox": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "detox": "^20.18.3",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "typescript": "~5.1.3",
    "expect": "^29.7.0",
    "pretty-format": "^29.7.0",
    "ajv": "^8.17.1",
    "bunyan": "^1.8.15",
    "bunyan-debug-stream": "^3.1.0",
    "caf": "^15.0.1",
    "child-process-promise": "^2.2.1",
    "detox-copilot": "^0.0.24",
    "execa": "^5.1.1",
    "fs-extra": "^11.2.0",
    "funpermaproxy": "^1.1.0",
    "glob": "^8.1.0",
    "ini": "^1.3.8",
    "jest-environment-emit": "^1.0.8",
